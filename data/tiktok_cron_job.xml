<openerp>
    <data>
        <record id="ir_cron_tiktok_cron_job" model="ir.cron" forcecreate="True">
            <field name="name">Tiktok Cron Job</field>
            <field name="model_id" ref="model_tiktok_post"/>
            <field name="state">code</field>
            <field name="code">model.reset_cover_img()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">6</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="True"/>
        <!-- <field name="nextcall" eval="(DateTime.now()).strftime('%Y-%m-%d 00:00:01')"/>-->
        </record>
    </data>
</openerp>